{
  "name": "xjx",
  "version": "1.0.0",
  "description": "A modular XML/JSON transformation library with fluent API",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./transforms": {
      "import": "./dist/esm/transforms/index.js",
      "require": "./dist/cjs/transforms/index.js",
      "types": "./dist/types/transforms/index.d.ts"
    },
    "./full": {
      "import": "./dist/esm/xjx.full.js",
      "require": "./dist/cjs/xjx.full.js",
      "types": "./dist/types/xjx.full.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build:js": "rollup -c",
    "build:prod": "NODE_ENV=production npm run build:js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "keywords": [
    "xml",
    "json",
    "convert",
    "transform",
    "xml-parser",
    "json-parser"
  ],
  "license": "MIT",
  "sideEffects": [
    "**/extensions/*.js",
    "**/extensions/*.ts",
    "**/index.js",
    "**/index.ts",
    "dist/esm/index.js",
    "dist/cjs/index.js",
    "dist/esm/extensions/*.js",
    "dist/cjs/extensions/*.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "babel-jest": "^29.7.0",
    "eslint": "^8.37.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^4.4.1",
    "rollup": "^3.29.5",
    "rollup-plugin-filesize": "^10.0.0",
    "ts-jest": "^29.3.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "jsdom": "^21.1.1"
  },
  "peerDependenciesMeta": {
    "jsdom": {
      "optional": true
    },
    "@xmldom/xmldom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.16.0"
  }
}